{
  "name": "JWGrabber",
  "description": "grab jw source urls!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting","storage","tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Click"
  }
}
